#include <iostream>
using namespace std;

class Rectangle {
private:
    double length;
    double width;

public:

    void setDimension(double l, double w) {
        length = l;
        width = w;
    }

    double calculateArea() {
        return length * width;
    }


    void displayArea() {
        cout << "The area of the rectangle is: " << calculateArea() << endl;
    }
};

int main() {
    Rectangle kotak;
    kotak.setDimension(3.4, 4.5);
    kotak.displayArea();
    return 0;
}
